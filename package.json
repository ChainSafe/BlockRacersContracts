{
  "name": "BlockGameContracts",
  "version": "2.0.0",
  "main": "index.js",
  "repository": "git@github.com:ChainSafe/BlockRacersContracts.git",
  "author": "ChainSafe Systems, RyRy79261 <ryanjnoble@gmail.com>, Oleksii Matiiasevych <oleksii@chainsafe.io>",
  "license": "MIT",
  "scripts": {
    "node": "npx hardhat node",
    "deploy": "npx hardhat run scripts/deploy.ts",
    "deploy-dev": "npx hardhat --network localhost run scripts/deploy.ts",
    "deploy-sepolia": "npx hardhat --network sepolia run scripts/deploy.ts",
    "deploy-mumbai": "npx hardhat --network mumbai run scripts/deploy.ts",
    "deploy-cronos-testnet": "npx hardhat --network cronos-testnet run scripts/deploy.ts",
    "deploy-helper-cronos-testnet": "npx hardhat --network cronos-testnet run scripts/deployUIHelper.ts",
    "set-base-uri-cronos-testnet": "npx hardhat --network cronos-testnet run scripts/setBaseUri.ts",
    "generate-metadata": "npx hardhat run scripts/generateMetadata.ts",
    "test": "npx hardhat test",
    "compile": "npx hardhat compile && npx yarn generateUnityClasses",
    "generateUnityClasses": "npx ts-node --files ./scripts/generateUnityClasses.ts",
    "format": "prettier --write '*/**/*.*{js,json,md,ts}'",
    "format:check": "prettier --check '*/**/*.*{js,json,md,ts}'",
    "lint": "eslint --cache . && yarn lint:sol",
    "lint:ts": "eslint -c .eslintrc.json --ext \"**/*.ts\" \"**/*.test.ts\"",
    "lint:sol": "solhint 'contracts/**/*.sol'"
  },
  "devDependencies": {
    "@gelatonetwork/relay-sdk": "^5.5.5",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@openzeppelin/contracts": "^5.0.0",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": ">=9.1.0",
    "@types/node": ">=16.0.0",
    "chai": "^4.2.0",
    "dotenv": "^16.4.4",
    "ethers": "^6.4.0",
    "hardhat": "^2.19.1",
    "hardhat-gas-reporter": "^1.0.8",
    "js-combinatorics": "^2.1.2",
    "nethereum-codegen": "1.0.26",
    "solidity-coverage": "^0.8.0",
    "ts-node": ">=8.0.0",
    "typechain": "^8.3.0",
    "typescript": ">=4.5.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.0",
    "prettier-plugin-solidity": "^1.3.1",
    "solhint": "^4.1.1",
    "solhint-plugin-prettier": "^0.1.0"
  }
}
